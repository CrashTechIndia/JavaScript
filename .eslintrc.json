{
	"env": {
		"browser": true,
		"es2021": true,
		"node": true,
		"jquery": true
	},
	"extends": ["airbnb-base"],
	"parserOptions": {
		"ecmaVersion": 12
	},
	"rules": {
		"linebreak-style": 0,
		"no-console": 0,
		"indent": 0,
		"quotes": ["warn", "double"],
		"comma-dangle": 0,
		"no-tabs": 0
	}
}
